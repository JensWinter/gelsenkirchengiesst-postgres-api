{
	"exclude": [
		"node_modules",
		"api/__tests__"
	],
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"outDir": "./dist",
		"rootDir": "api",
		"strict": true,
		"typeRoots": [
			"node_modules/@types"
		],
		"types": [
			"node",
			"jest"
		],
		"esModuleInterop": true,
		"moduleResolution": "node",
		"forceConsistentCasingInFileNames": true
	}
}